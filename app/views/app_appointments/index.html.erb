<p id="notice"><%= notice %></p>

<h1>App Appointments</h1>

<table>
  <thead>
    <tr>
      <th>Appointmentbody</th>
      <th>Datentime</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @app_appointments.each do |app_appointment| %>
      <tr>
        <td><%= app_appointment.AppointmentBody %></td>
        <td><%= app_appointment.DatenTime %></td>
        <td><%= link_to 'Show', app_appointment %></td>
        <td><%= link_to 'Edit', edit_app_appointment_path(app_appointment) %></td>
        <td><%= link_to 'Destroy', app_appointment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New App Appointment', new_app_appointment_path %>
